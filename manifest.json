{
  "name": "RandPlay Bookmark",
  "version": "1.0",
  "description": "randomly play your bookmark",
  "permissions": [
    "tabs",
    "bookmarks",
    "webNavigation",
    "https://www.nicovideo.jp/*",
    "https://www.youtube.com/*"
  ],
  "background": {
    "page":"background.html",
    "persistent": true
  },
  "web_accessible_resources": [
    "randomplay.js",
    "youtube_controller.js"
 ],
  "browser_action": {},
  "manifest_version": 2,
  "options_page": "folder_option.html"
}