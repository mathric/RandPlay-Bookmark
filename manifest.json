{
  "name": "RandPlay Bookmark",
  "version": "1.0",
  "description": "randomly play your bookmark",
  "permissions": [
    "tabs",
    "bookmarks",
    "https://www.nicovideo.jp/*"
  ],
  "background": {
    "scripts": [
      "randomplay.js"
    ],
    "persistent": false
  },
  "browser_action": {},
  "manifest_version": 2
}